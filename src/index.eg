
require:
   node-emoji -> emoji

install{@, options} =
   @register-rules with {
      "!! \\name"{engine, {=> name}} =
         emoji[name.shed-all{}.raw{}.trim{}]
   }

main{*match} =
   {@, options = {=}} when @is-quaint-engine ->
      install{@, options}
   {options = {=}} ->
      {@} -> main{@, options}

provide = main

